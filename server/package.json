{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "TZ=Europe/Moscow nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --loader ./ts-node-with-paths-loader.js --no-warnings node_modules/typeorm/cli.js -d ./src/database/datasource.ts",
    "typeorm:migration:generate": "pnpm run typeorm migration:generate",
    "typeorm:migration:create": "pnpm typeorm migration:create ./src/database/migrations/${0}",
    "typeorm:migration:run": "pnpm typeorm migration:run",
    "typeorm:migration:revert": "pnpm typeorm migration:revert",
    "typeorm:schema:drop": "pnpm typeorm schema:drop",
    "proto": "protoc --plugin=node_modules/.bin/protoc-gen-ts_proto -I=../microservice_contracts/proto --ts_proto_out=src/microservices/proto --ts_proto_opt=nestJs=true --ts_proto_opt=fileSuffix=.pb --ts_proto_opt=addGrpcMetadata=true",
    "proto:all": "pnpm proto ../microservice_contracts/proto/erp.proto ../microservice_contracts/proto/erp_ozon.proto"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@bull-board/api": "^5.8.4",
    "@bull-board/express": "^5.8.4",
    "@bull-board/nestjs": "^5.8.4",
    "@eventstore/db-client": "^6.2.1",
    "@grpc/grpc-js": "^1.10.7",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/typeorm": "^10.0.0",
    "@type-ddd/core": "^0.0.5",
    "@types/ms": "^0.7.34",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.12.3",
    "cache-manager": "^5.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "got": "^13.0.0",
    "graphql": "^16.8.1",
    "graphql-type-json": "^0.3.2",
    "hbs": "^4.2.0",
    "joi": "^17.11.0",
    "json-case-convertor": "^1.4.4",
    "lodash": "^4.17.21",
    "mongoose": "^8.4.4",
    "ms": "^2.1.3",
    "nestjs-cls": "^4.3.0",
    "nestjs-telegraf": "^2.7.0",
    "node-cache": "^5.1.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
