    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "telegraf": "^4.14.0",
    "tough-cookie": "^4.1.3",
    "ts-case-convert": "^2.0.7",
    "ts-morph": "^21.0.0",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.1"
